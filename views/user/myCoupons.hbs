 <style>
  .paadding{
    padding-left: 132px;
    padding-top: 10px;

}
@media  (max-width: 991px) {
    .paadding{
        padding:15px;
    }
   }


.coupon-card {
    border: 0.5px solid #d4d5d9;
    // -webkit-border-radius: 2px;
    // -moz-border-radius: 2px;
    border-radius: 2px;
    background-color: white;
   padding: 0% 2%;
    margin-bottom: 10px;
    color: #282C3F;
}
.coupon-off-component {
    display: inline-block;
    vertical-align: top;
    text-align: center;
    width: 29%;
    max-width: 100px;
    color: #14cda8;
    border-right: 1px solid #d4d5d9;
}
.coupon-amount {
 margin: 30px 0px 4px 0px;
    font-weight: 700;
    font-size: 16px;
}
.coupon-offText {
    font-weight: 500;
    font-size: 18px;
}
.coupon-info {
    padding: 20px 0px 10px 10px;
    display: inline-block;
    width: 64%;
}
.coupon-purchase-info {
 margin: 10px 0px;
}
.coupon-label {
    color: #7e818c;
}
.coupon-coupon-code {
    text-transform: uppercase;
    word-wrap: break-word;
}
.coupon-coupon-detail {
   border-top: 1px solid #d4d5d9;
    padding: 15px 0px;
}
.coupon-expiry-date {
    font-weight: 600;
    color: #696E79;
    border-right: 1px solid #d4d5d9;
    padding-right: 5px;
}
// .coupon-details {
    
//     float: right;
//     font-weight: 500;
//     color: #526cd0;
// }
.couponDetail{
    float: right;
    border: none;
    background-color:white;
    font-weight: 500;
}
 </style>
 {{>userHeader}}
  
			<div class="container">
	<div class="container mt-3 mt-md-5" style="background-color: rgb(255, 240, 252);">
  <h2 class="text-charcoal hidden-sm-down mb-2">MyCoupons</h2>
  <div class="main-panel">
          <div class="content-wrapper">
            <div class="page-header">
            
              
            </div>
            <div class="row">
             
              <div class="col-lg-12 grid-margin stretch-card">
                <div class="card">
                  <div class="card-body">
                   
                    {{!-- <p class="card-description"> Add class <code>.table-hover</code>
                    </p> --}}
                     {{#each coupons}}
  <div class="coupon-card" key={{index}}>

                                <div class="coupon-off-component sprites-couponBackGroundImage sprites-mymyntraCouponSprite">
                                    <div class="coupon-amount">
                                       {{this.discount_value}}
                                    </div>
                                    <div class="coupon-offText">OFF </div>
                                </div>
                                <div class="coupon-info">
                                    <div class="coupon-purchase-info">
                                        <span class="coupon-label">{{this.coupon_name}}</span>
                                  
                                    </div>
                                    <div>
                                        <span class="coupon-label"> Code: </span>
                                        <span class="coupon-coupon-code">

                                           {{this.coupon_code}}
                                        </span>
                                    </div>
                                </div>

                                <div class="coupon-coupon-detail">
                                  <div> Description :  {{this.description}}</div>
                                    <span>Expiry: </span>
                                    <span class="coupon-expiry-date">{{this.valid_till}}</span>
                                </div>

                            </div>
{{/each}}

                    <div class="table-responsive">
                      <table class="table table-hover">
                       
                        <tbody>
   
   
  </tbody>
                      </table>
                    </div>
                  </div>
                </div>
              </div>
            
             
              
            </div>
          </div>
          
        </div>
  </div>

   </div>
  